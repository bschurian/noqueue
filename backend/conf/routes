# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# TEST PAGE
GET           /test                               controllers.Application.test
GET           /setup                               controllers.Application.setup
GET           /test2                                controllers.Application.test2
GET           /test3                                controllers.Application.test3


# SETUP OF DATABASE
GET           /setup                              controllers.Application.setup

# AUTHENTICATION
#POST          /signin                             controllers.Auth.signIn
#POST          /signout                            controllers.Auth.signOut
#POST          /signup                             controllers.Auth.signUp

# ANWENDER
POST          /anwender                           controllers.AnwenderC.create
GET             /anwender/:id                     controllers.AnwenderC.get(id: Long)



# Map static resources from the /public folder to the /assets URL path
GET           /assets/*file                       controllers.Assets.versioned(path="/public", file: Asset)
